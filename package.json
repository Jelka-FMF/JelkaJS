{
  "name": "jelka",
  "description": "JavaScript API for Jelka FMF patterns",
  "license": "MIT",
  "version": "0.2.0",
  "homepage": "https://github.com/Jelka-FMF/JelkaJS",
  "repository": "https://github.com/Jelka-FMF/JelkaJS",
  "bugs": "https://github.com/Jelka-FMF/JelkaJS",
  "author": "Programerski klub FMF",
  "type": "module",
  "main": "_dist/index.js",
  "module": "_dist/index.js",
  "types": "_dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./_dist/index.d.ts",
      "default": "./_dist/index.js"
    },
    "./*": {
      "types": "./_dist/*.d.ts",
      "default": "./_dist/*.js"
    }
  },
  "files": [
    "_dist"
  ],
  "scripts": {
    "clean": "rimraf ./_dist",
    "build": "run-s clean build:*",
    "build:swc": "swc src --out-dir _dist --strip-leading-paths",
    "build:tsc": "tsc",
    "watch": "run-p watch:*",
    "watch:swc": "swc src --out-dir _dist --strip-leading-paths --watch",
    "watch:tsc": "tsc --watch",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --check src",
    "fix": "run-s -c fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write src",
    "docs": "typedoc"
  },
  "packageManager": "npm@10.8.1",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/node": "^22.10.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typedoc": "^0.27.4",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  }
}
