{
  "name": "jelka",
  "description": "JavaScript API for Jelka FMF patterns",
  "license": "MIT",
  "version": "0.3.0",
  "homepage": "https://github.com/Jelka-FMF/JelkaJS",
  "repository": "https://github.com/Jelka-FMF/JelkaJS",
  "bugs": "https://github.com/Jelka-FMF/JelkaJS",
  "author": "Programerski klub FMF",
  "type": "module",
  "main": "_dist/index.js",
  "module": "_dist/index.js",
  "types": "_dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./_dist/index.d.ts",
      "default": "./_dist/index.js"
    },
    "./*": {
      "types": "./_dist/*.d.ts",
      "default": "./_dist/*.js"
    }
  },
  "files": [
    "_dist"
  ],
  "scripts": {
    "clean": "rimraf ./_dist",
    "build": "run-s clean build:*",
    "build:swc": "swc src --out-dir _dist --strip-leading-paths",
    "build:tsc": "tsc",
    "watch": "run-p watch:*",
    "watch:swc": "swc src --out-dir _dist --strip-leading-paths --watch",
    "watch:tsc": "tsc --watch",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint src",
    "lint:prettier": "prettier --check src",
    "fix": "run-s -c fix:*",
    "fix:eslint": "eslint --fix src",
    "fix:prettier": "prettier --write src",
    "docs": "typedoc"
  },
  "packageManager": "npm@11.7.0",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}
